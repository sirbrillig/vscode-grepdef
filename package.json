{
	"name": "vscode-grepdef",
	"displayName": "vscode-grepdef",
	"description": "VSCode extension for grepdef: search for symbol definitions in various programming languages",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-grepdef.grepdefword",
		"onCommand:vscode-grepdef.grepdef"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-grepdef.grepdefword",
				"title": "GrepDef for word under cursor"
			},
			{
				"command": "vscode-grepdef.grepdef",
				"title": "GrepDef"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"prettier": {
		"trailingComma": "es5",
		"semi": true,
		"useTabs": true,
		"singleQuote": true
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"prettier": "^2.0.5"
	}
}
